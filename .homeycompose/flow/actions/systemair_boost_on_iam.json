{
  "id": "systemair_boost_on_iam",
  "title": {
    "en": "Initiate boost mode"
  },
  "hint": {
    "en": "The boost mode overrides the selected fan speed to 'High' for a period."
  },
  "args": [
    {
      "name": "device",
      "type": "device",
      "filter": "driver_id=SystemairIAM"
    },
    {
      "name": "boost_period",
      "type": "range",
      "min": 1,
      "max": 240,
      "step": 1,
      "label": "min",
      "labelDecimals": 0,
      "title": {
        "en": "Minutes"
      }
    }
  ]
}
